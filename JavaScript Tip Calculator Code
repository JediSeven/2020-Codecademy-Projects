// This function will tell the person how much he/she should tip by multiplying the total bill by a percentage that is determined based on the stated quality.
const tipCalculator = (quality, total) => {
  switch (quality){
    case 'bad':
    return total * .05
  break
    case 'ok':
    return total * .15
  break
    case 'good':
    return total * .20
  break
    case 'excellent':
    return total * .30
  break
    default:
    return total * .18
  }
}

//This line of code is used to test the function above and should return 20.
console.log(tipCalculator('good', 100))
